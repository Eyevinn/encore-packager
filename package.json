{
  "name": "@eyevinn/encore-packager",
  "version": "0.11.0",
  "description": "The default typescript-nodejs template for eyevinn projects",
  "bin": {
    "encore-packager": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "jest",
    "prepare": "husky install",
    "lint": "eslint .",
    "pretty": "prettier --check --ignore-unknown .",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "dev": "nodemon",
    "start": "ts-node -T src/cli.ts",
    "postversion": "git push && git push --tags"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.15.0"
  },
  "dependencies": {
    "@eyevinn/shaka-packager-s3": "^0.6.3",
    "@fastify/cors": "^8.2.0",
    "@fastify/type-provider-typebox": "^2.4.0",
    "@osaas/client-core": "^0.8.0",
    "@sinclair/typebox": "^0.25.24",
    "commander": "^12.1.0",
    "fastify": "^4.12.0",
    "nodemon": "^3.1.4",
    "redis": "^4.6.14",
    "ts-node": "^10.9.1",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.0"
  }
}
